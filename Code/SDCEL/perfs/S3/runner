#!/bin/bash

COMMAND="sdcel2_debug"
DATASET="Test/S3"

PS=(100 250)
for n in $(seq 1 $1)
do
    for P in "${PS[@]}"
    do
	echo "Run $n ./$COMMAND $DATASET/P$P /home/acald013/RIDIR/local_path/$DATASET/P$P/ 1e-6 $P"
	./$COMMAND $DATASET/P$P /home/acald013/RIDIR/local_path/$DATASET/P$P/ 1e-6 $P	
    done
done
